
<div class="col-md-12">
    <table class="col-md-6 table table-bordered table-striped myTable table-hover">
        <thead class="">
            <tr class="label-primary">
                <th ng-click="sortList('Name')" class="col-md-2"><span class="text-white">Name </span><div ng-class="getListSortClass('Name')"></div></th>
                <th ng-click="sortList('Strength')" class="col-md-1"><span class="text-white">Strength </span><div ng-class="getListSortClass('Strength')"></div></th>
                <th ng-click="sortList('Health')" class="col-md-1"><span class="text-white">Health </span><div ng-class="getListSortClass('Health')"></div></th>
                <th ng-click="sortList('Dexterity')" class="col-md-2"><span class="text-white">Dexterity </span><div ng-class="getListSortClass('Dexterity')"></div></th>
                <th ng-click="sortList('LuckModifier')" class="col-md-2"><span class="text-white">Luck </span><div ng-class="getListSortClass('LuckModifier')"></div></th>
                <th ng-click="sortList('DefenceModifier')" class="col-md-2"><span class="text-white">DefMod </span><div ng-class="getListSortClass('DefenceModifier')"></div></th>
                <th ng-click="sortList('StrengthModifier')" class="col-md-2"><span class="text-white">StrMod </span><div ng-class="getListSortClass('StrengthModifier')"></div></th>
            </tr>

        </thead>
        <tbody>
            <tr ng-repeat="s in statics | limitTo : 20:0 | orderBy : sortListColumn : reverseListSort" class="" ng-click="changeDetail(s.StaticCharacterId)">
                <td class="col-md-2">{{s.Name}}</td>
                <td class="col-md-1">{{s.Strength}}</td>
                <td class="col-md-1">{{s.Health}}</td>
                <td class="col-md-2">{{s.Dexterity}}</td>
                <td class="col-md-2">{{s.LuckModifier}}</td>
                <td class="col-md-2">{{s.DefenceModifier}}</td>
                <td class="col-md-2">{{s.StrengthModifier}}</td>
            </tr>
        </tbody>
    </table>
</div>

